local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/QuackyCoderDuck/Library/main/Key%20System')))()
local Player = game.Players.LocalPlayer
local Window = OrionLib:MakeWindow(Name = "Key System", HidePremium = false, Save Config = true, IntroEnabled = true})

OrionLib:MakeNotification({
	Name = "Logged in!",
	Content = "Welcome,"..Player.Name.."Please Enter Key.",
	Image = "rbxassetid://4483345998",
	Time = 5
})

_G.Key = "TEST_KEY"
_G.KeyInput = "string"

function MakeScriptHub()
        print("Correct Key")
   end
   function CorrectKeyNotifcation()
 OrionLib:MakeNotification({
	Name = "Corect Key! Loading...",
	Content = "Please wait while the hub loads!",
	Image = "rbxassetid://4483345998",
	Time = 5
          --  loadstring(game:HttpGet("https://raw.githubusercontent.com/QuackyCoderDuck/RHUBV2/main/Hub"))() 
                OrionLib:MakeNotification({--
	Name = "Loaded!",
	Content = "Hub has loaded, enjoy!",
	Image = "rbxassetid://4483345998",
	Time = 5
        OrionLib:Destroy()
})
end
function InncorrectKey()
 OrionLib:MakeNotification({
	Name = "Incorrect Key!",
	Content = "Please join the discord server in order to get the key!! 
        https://discord.gg/sruX5ArsAn",
	Image = "rbxassetid://4483345998",
	Time = 5
})
end
     
           

local Tab = Window:MakeTab({
	Name = "Key",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddTextbox({
	Name = "Enter Key --->",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		_G.KeyInput = Value
	end	  
})

Tab:AddButton({
	Name = "Check Key",
	Callback = function()
      		if _G.KeyInput == _G.Key then
               MakeScriptHub()
               CorrectKeyNotification()
               else
               InncorrectKeyNotification()
                 end      
  	end    
})
