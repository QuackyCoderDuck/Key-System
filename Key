        LoadingSubtitle = "by marker", 
        ConfigurationSaving = {
          Enabled = false,
          FolderName = nil,
          FileName = "Marker's Hub:
       },
       Discord = {
       Enabled = true,
       Invite = "PQa6dJq8yT"
       RememberJoins = "true
       },
       KeySystem = false,
       KeySettings = {
       Title = "Marker's Hub",
       Subtitle = "Key System",
       Note = "Join the Discord Server! (discord.gg/PQa6dJq8yT)",
       FileName = "MarkerKey",
       SaveKey = true,
       GrabKeyFromSite = false,
       Key = "Hello"
       }
     })
end

function readKey()
 if readfile("/markerKey/key.txt") == loadedKey then
